<p class="lead titled">My Own Feedback Requests</p>
<ul class="spotless glower-mini">
<% sentFeedbackRequests.each(function (sentFeedbackRequest) { %>
	<li class="well lead">
    <a href="<%= '#/feedback_requests/' + sentFeedbackRequest.id %>">
			<%= sentFeedbackRequest.escape('body') %>
		</a>
	</li>
<% }); %>
</ul>